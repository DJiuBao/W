<h1>W3School.com.cn Internal Note</h1>
<p>
<b>TITLE:</b> <span id="to"></span><br />
<b>ARTIST:</b> <span id="from"></span><br />
<b>PRICE:</b> <span id="message"></span>
</p>

<script type = "text/javascript">
if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  xmlhttp = new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
  }
xmlhttp.open("GET","/xml2js.xml",false);
xmlhttp.send();
xmlDoc = xmlhttp.responseXML;

document.getElementById("to").innerHTML=
xmlDoc.getElementsByTagName("TITLE")[0].childNodes[0].nodeValue;
document.getElementById("from").innerHTML=
xmlDoc.getElementsByTagName("ARTIST")[0].childNodes[0].nodeValue;
document.getElementById("message").innerHTML=
xmlDoc.getElementsByTagName("PRICE")[0].childNodes[0].nodeValue;
</script>